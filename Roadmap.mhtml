.team_social a { color: rgb(255, 255, 255); font-size: 18px; display: inline-block; background: rgb(204, 204, 204); width: 35px; height: 35px; line-height: 35px; border-radius: 50%; transition: all 0.5s ease 0s; }

#pricing { background: none 0px 0px repeat scroll rgb(248, 248, 248); display: inline; float: left; padding: 80px 0px; width: 100%; border-top: 1px solid rgb(238, 238, 238); border-bottom: 1px solid rgb(238, 238, 238); }

