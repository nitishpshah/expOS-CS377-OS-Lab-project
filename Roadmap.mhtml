.single_price > h3 { font-size: 24px; font-weight: 700; line-height: 100px; margin: 0px; text-transform: uppercase; }

.single_price .price { color: rgb(255, 255, 255); padding: 15px 0px; }

.single_price .price > h4 { font-size: 60px; font-weight: 700; line-height: 60px; margin: 0px 0px 5px; }

.single_price .price > span { font-size: 15px; }

.price_features { padding: 10px 25px; }

.price_features li { border-bottom: 1px solid rgb(242, 242, 242); display: block; font-size: 15px; line-height: 18px; padding: 10px 0px; text-transform: uppercase; }

.price_btn { display: inline-block; font-size: 15px; font-weight: bold; margin-bottom: 20px; margin-top: 10px; padding: 10px 20px; text-transform: uppercase; transition: all 0.5s ease 0s; color: rgb(255, 255, 255) !important; }

.price_btn:hover { text-decoration: none; outline: none; background-color: rgb(255, 255, 255); }

.business_price { border: 1px solid rgb(255, 255, 255); color: rgb(255, 255, 255); }

.business_price .price { border-bottom: 3px solid rgb(255, 255, 255); border-top: 3px solid rgb(255, 255, 255); }

.business_price .price_btn { border-color: rgb(255, 255, 255); background-color: rgb(255, 255, 255); }

