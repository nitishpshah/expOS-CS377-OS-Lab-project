.blog_slider .slick-prev:hover, .blog_slider .slick-next:hover { opacity: 0.7; }

#testimonial { background: none 0px 0px repeat scroll rgb(248, 248, 248); display: inline; float: left; padding: 80px 0px; width: 100%; border-top: 1px solid rgb(238, 238, 238); border-bottom: 1px solid rgb(238, 238, 238); }

